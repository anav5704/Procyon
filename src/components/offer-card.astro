---
import CircleDesigns from "./circle-designs.astro";
const {name, svg} = Astro.props

---

<div class="relative h-full rounded-md bg-procyon-highlight-purple aspect-square p-3 overflow-clip border border-purple-500
   z-10">
    <div class="absolute">
        <img src={svg} alt="" class="w-8 h-8 ">
    </div>
    <p class="text-white text-2xl font-extrabold h-min mb-auto mt-16">
        {name}
    </p>
    
    <CircleDesigns></CircleDesigns>
</div>

